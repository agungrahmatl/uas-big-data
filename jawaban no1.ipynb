   "source": [
    "rfmTable = data.groupby('CardID').agg({'hist': lambda x:x.min(), # Recency\n",
    "                                        'CardID': lambda x: len(x), # Frequency\n",
    "                                        'Amount': lambda x: x.sum()}) # Monetary Value\n",
